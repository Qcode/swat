<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title">SwatColorEntry Demos</property>
		<widget class="SwatForm" id="my_form">

			<widget class="DemoDocumentationLink">
				<property name="title">SwatColorEntry Documentation</property>
				<property name="link">http://docs.silverorange.com/Swat/SwatColorEntry.html</property>
			</widget>

			<widget class="SwatFormField">
				<property name="title">Complex Color Entry</property>
				<widget class="SwatColorEntry" id="color_entry" />
			</widget>

			<widget class="DemoDocumentationLink">
				<property name="title">SwatSimpleColorEntry Documentation</property>
				<property name="link">http://docs.silverorange.com/Swat/SwatSimpleColorEntry.html</property>
			</widget>

			<widget class="SwatFormField">
				<property name="title">Simple Color Entry</property>
				<widget class="SwatSimpleColorEntry" id="simple_color_entry">
					<property name="value">eed680</property>
				</widget>
			</widget>

		</widget>
	</widget>
</swat-interface>
