<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title">SwatTimeZoneEntry Demo</property>
		<widget class="SwatForm" id="my_form">

			<widget class="DemoDocumentationLink">
				<property name="title">SwatTimeZoneEntry Documentation</property>
				<property name="link">http://docs.silverorange.com/Swat/SwatTimeZoneEntry.html</property>
			</widget>

			<widget class="SwatFormField">
				<property name="title">Select Your Timezone</property>
				<widget class="SwatTimeZoneEntry" id="timezone" />
			</widget>

		</widget>
	</widget>
</swat-interface>
