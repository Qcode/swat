<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title">SwatCheckbox Demos</property>
		<widget class="SwatForm" id="my_form">

			<widget class="SwatFieldset">
				<property name="title">Individual Checkboxes</property>

				<widget class="DemoDocumentationLink">
					<property name="title">SwatCheckbox Documentation</property>
					<property name="link">http://docs.silverorange.com/Swat/SwatCheckbox.html</property>
				</widget>

				<widget class="SwatFormField">
					<property name="title">Check 1</property>
					<widget class="SwatCheckbox" id="check1">
					</widget>
				</widget>
				<widget class="SwatFormField">
					<property name="title">Check 2</property>
					<widget class="SwatCheckbox" id="check2">
					</widget>
				</widget>
				
			</widget>
			
			<widget class="SwatFieldset">
				<property name="title">Checkbox List</property>

				<widget class="DemoDocumentationLink">
					<property name="title">SwatCheckboxList Documentation</property>
					<property name="link">http://docs.silverorange.com/Swat/SwatCheckboxList.html</property>
				</widget>

				<widget class="SwatCheckboxList" id="checkbox_list">
					<property name="options[0]">Apple</property>
					<property name="options[1]">Orange</property>
					<property name="options[2]">Banana</property>
					<property name="options[3]">Pear</property>
					<property name="options[4]">Pineapple</property>
					<property name="options[5]">Kiwi</property>
				</widget>
				
			</widget>
			<widget class="SwatFieldset">
				<property name="title">Checkbox Tree</property>

				<widget class="DemoDocumentationLink">
					<property name="title">SwatCheckboxTree Documentation</property>
					<property name="link">http://docs.silverorange.com/Swat/SwatCheckboxTree.html</property>
				</widget>

				<widget class="SwatCheckboxTree" id="checkbox_tree">
				</widget>
				
			</widget>

		</widget>
	</widget>
</swat-interface>
