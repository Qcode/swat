<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title" value="SwatCheckbox and SwatCheckboxList Examples" />
		<widget class="SwatForm" id="my_form">

			<widget class="SwatFieldset">
				<property name="title" value="Individual Checkboxes" />

				<widget class="SwatFormField">
					<property name="title" value="Check 1" />
					<widget class="SwatCheckbox" id="check1">
					</widget>
				</widget>
				<widget class="SwatFormField">
					<property name="title" value="Check 2" />
					<widget class="SwatCheckbox" id="check2">
					</widget>
				</widget>
				
			</widget>
			
			<widget class="SwatFieldset">
				<property name="title" value="Checkbox List" />

				<widget class="SwatCheckboxList" id="checkbox_list">
					<property name="options[0]" value="Apple" />
					<property name="options[1]" value="Orange" />
					<property name="options[2]" value="Banana" />
					<property name="options[3]" value="Pear" />
					<property name="options[4]" value="Pineapple" />
					<property name="options[5]" value="Kiwi" />
				</widget>
				
			</widget>
			<widget class="SwatFieldset">
				<property name="title" value="Checkbox Tree" />

				<widget class="SwatCheckboxTree" id="checkbox_tree">
				</widget>
				
			</widget>

		</widget>
	</widget>
</swat-interface>
