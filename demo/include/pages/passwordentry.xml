<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title" value="SwatPasswordEntry Demo" />
		<widget class="SwatForm" id="my_form">
			<property name="action" value="index.php?demo=PasswordEntry" />

			<widget class="SwatFormField">
				<property name="title" value="Password Entry" type="string" />
				<widget class="SwatPasswordEntry" id="password">
					<property name="required" value="true" type="boolean" />
				</widget>
			</widget>

			<widget class="SwatFormField">
				<property name="title" value="Confirm Password Entry" type="string" />
				<widget class="SwatConfirmPasswordEntry" id="confirm_password">
				</widget>
			</widget>

			<widget class="SwatFormFooterField">
				<widget class="SwatButton">
					<property name="title" value="Validate" type="string" />
				</widget>
			</widget>
		</widget>
	</widget>
</swat-interface>
