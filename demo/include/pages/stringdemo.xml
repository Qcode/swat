			<property name="title" value="Right Ellipsize" />
			<property name="open" value="false" type="boolean" />
			<widget class="SwatContentBlock" id="right_ellipsize">
			</widget>
		</widget>
		<widget class="SwatDisclosure">
			<property name="title" value="Middle Ellipsize" />
			<property name="open" value="false" type="boolean" />
