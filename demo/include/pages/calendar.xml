<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title">SwatCalendar Demo</property>
		<widget class="SwatForm" id="my_form">

			<widget class="DemoDocumentationLink">
				<property name="title">SwatCalendar Documentation</property>
				<property name="link">http://docs.silverorange.com/Swat/SwatCalendar.html</property>
			</widget>

			<widget class="SwatFormField">
				<property name="title" type="string">Select a Date</property>
				<widget class="SwatCalendar" id="calendar" />
			</widget>
				
		</widget>
	</widget>
</swat-interface>
