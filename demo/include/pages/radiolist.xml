<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title">SwatRadioList Demo</property>
		<widget class="SwatForm" id="my_form">

			<widget class="DemoDocumentationLink">
				<property name="title">SwatRadioList Documentation</property>
				<property name="link">http://docs.silverorange.com/Swat/SwatRadioList.html</property>
			</widget>

			<widget class="SwatFormField">
				<property name="title" type="string">Select an Option</property>
				<widget class="SwatRadioList" id="radio_list">
					<property name="options[0]">Apple</property>
					<property name="options[1]">Orange</property>
					<property name="options[2]">Banana</property>
					<property name="options[3]">Pear</property>
					<property name="options[4]">Pineapple</property>
					<property name="options[5]">Kiwi</property>
					<property name="options[6]">Tangerine</property>
					<property name="options[7]">Grapefruit</property>
					<property name="options[8]">Strawberry</property>
					<property name="value" type="integer">7</property>
				</widget>
			</widget>

		</widget>
	</widget>
</swat-interface>
