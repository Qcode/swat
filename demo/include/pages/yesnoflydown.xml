<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title" value="SwatYesNoFlydown Demos" />
		<widget class="SwatForm" id="my_form">

			<widget class="SwatFormField">
				<property name="title" value="Default" />
				<widget class="SwatYesNoFlydown" id="yes_no_default" />
			</widget>

			<widget class="SwatFormField">
				<property name="title" value="No blank, defaults to 'Yes'" />
				<widget class="SwatYesNoFlydown" id="yes_no_noblank">
					<property name="show_blank" value="false" type="boolean" />
					<property name="value" value="YES" type="constant" />
				</widget>
			</widget>

			<widget class="SwatFormField">
				<property name="title" value="Defaults to 'No'" />
				<widget class="SwatYesNoFlydown" id="yes_no_no">
					<property name="value" value="NO" type="constant" />
				</widget>
			</widget>

		</widget>
	</widget>
</swat-interface>
