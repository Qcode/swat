<?xml version='1.0' standalone='yes'?>
<swat-interface>
	<widget class="SwatFrame" id="my_frame">
		<property name="title" value="SwatFlydown and SwatTreeFlydown Examples" />
		<widget class="SwatForm" id="my_form">

			<widget class="SwatFormField">
				<property name="title" value="Flat Flydown" type="string" />
				<widget class="SwatFlydown" id="flydown">
					<property name="blank_title" value="&lt; select a fruit >" />
				</widget>
			</widget>

			<widget class="SwatFormField">
				<property name="title" value="Tree Flyown" type="string" />
				<widget class="SwatTreeFlydown" id="tree_flydown">
					<property name="show_blank" value="false" type="boolean" />
				</widget>
			</widget>

		</widget>
	</widget>
</swat-interface>
