							<property name="title">Courtland</property>
							<property name="link">http://en.wikipedia.org/wiki/Cortland_%28apple%29</property>
						</widget>
						<widget class="SwatMenuItem">
							<property name="title">Macoun</property>
							<property name="link">http://en.wikipedia.org/wiki/Macoun_apple</property>
						</widget>
					</widget>
				</widget>
				</widget>
			</widget>
		</widget>
		<widget class="SwatMenuItem">
			<property name="title">Oranges</property>
			<widget class="SwatMenu">
				<widget class="SwatMenuItem">
					<property name="title">Blood</property>
