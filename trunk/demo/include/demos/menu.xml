	</widget>
	<widget class="SwatFrame">
		<property name="title">Toolbar Frame</property>
		<widget class="SwatToolbar">
			<widget class="SwatToolLink">
				<property name="title">Add</property>
				<property name="stock_id">create</property>
				<property name="link">#</property>
			</widget>

			<widget class="SwatToolLink">
				<property name="title">Delete</property>
				<property name="stock_id">delete</property>
				<property name="link">#</property>
			</widget>

			<widget class="SwatToolLink">
				<property name="title">Edit</property>
				<property name="stock_id">edit</property>
