			<property name="stock_id">add</property>
			<widget class="SwatGroupedMenu">
				<widget class="SwatMenuGroup">
					<property name="title">Bad Apples</property>
				<widget class="SwatMenuItem">
					<property name="stock_id">edit</property>
					<property name="title">Braeburn</property>
					<property name="link">http://en.wikipedia.org/wiki/Braeburn</property>
				</widget>
				<widget class="SwatMenuItem">
					<property name="title">Golden Delicious</property>
					<property name="link">http://en.wikipedia.org/wiki/Golden_Delicious</property>
				</widget>
				<widget class="SwatMenuItem">
					<property name="title">Granny Smith</property>
